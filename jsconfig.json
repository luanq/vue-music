{
    "compilerOptions": {
      "baseUrl": ".",
      "paths": {
        "@/*": ["src/*"],
        "@assets/*":["src/assets/*"],
        "@components/*": ["src/components/*"],
        "@utils/*": ["src/utils/*"],
        "@views/*": ["src/views/*"]
      },
      "allowJs": true
    },
    "exclude": ["node_modules", "dist"]
  }